{
  "name": "studiow/domino",
  "description": "Building blocks for event-driven domains",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Willem Wollebrants",
      "email": "willem@studiow.be",
      "homepage": "http://studiow.be",
      "role": "Developer"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*"
  },
  "repositories": [
  ],
  "autoload": {
    "psr-4": {
      "Studiow\\Domino\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Studiow\\Domino\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/php-cs-fixer fix"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpunit/phpunit": "^9.5"
  }
}
